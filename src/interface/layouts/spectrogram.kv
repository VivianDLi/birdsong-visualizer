#:kivy 2.1

<Spectrogram>:
    scrollview: scrollview
    on_size: self.on_spectrogram()
    ScrollView:
        id: scrollview
        size: root.size
        do_scroll_y: False
        always_overscroll: False
        effect_cls: kivy.effects.scroll.ScrollEffect
        Image:
            source: 'src/interface/resources/loading.gif'
            size_hint_x: None
